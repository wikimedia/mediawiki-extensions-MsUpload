{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"rules": {
		"camelcase": "warn",
		"no-jquery/variable-pattern": "warn",
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-animate-toggle": "off",
		"no-jquery/no-fade": "off",
		"no-jquery/no-done-fail": "warn"
	}
}
